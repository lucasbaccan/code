    "cspell": "cspell --no-summary --no-progress --config cspell.json \"**/*.{md,mdx}\"",
    "sc": "cspell --config cspell.yaml --show-suggestions \"**/docs/**/*.{md,mdx}\" | tee cspell.log",
    "scw": "cspell --config cspell.yaml --words-only --show-suggestions --unique \"**/docs/**/*.{md,mdx}\" | tee cspell.log"
  },
  "dependencies": {
